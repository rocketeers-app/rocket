{
    "name": "rocketeers-app/rocket",
    "description": "The Command Line Interface for Rocketeers",
    "keywords": ["rocketeers", "cli", "rocket"],
    "homepage": "https://rocketeers.app",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.5.8",
        "illuminate/http": "^8.83.23",
        "laminas/laminas-text": "^2.9",
        "laravel-zero/framework": "^8.10",
        "lorisleiva/laravel-actions": "^2.1.5",
        "spatie/ssh": "^1.7.2",
        "symfony/process": "^5.4.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5.23",
        "rector/rector": "^0.15.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/rocket"]
}
